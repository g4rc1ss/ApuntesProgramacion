@page
@model CleanArchitecture.Ejemplo.RazorPages.Pages.Users.UserDetailModel
@{
    Layout = "";
}

<div class="col-xs-12">
    <div class="form-group  col-md-3	">
        <label class="control-label" for="Id">Nombre</label>
    </div>
    <div class="form-group  col-md-6	">
        <input class="input-md form-control" value="@Model.UserResponse.Nombre" readonly>
    </div>

    <div class="form-group  col-md-3	">
        <label class="control-label" for="Id">Email</label>
    </div>
    <div class="form-group  col-md-6	">
        <input class="input-md form-control" value="@Model.UserResponse.Email" readonly>
    </div>

    <div class="form-group  col-md-3	">
        <label class="control-label" for="Id">Tiene Doble Factor</label>
    </div>
    <div class="form-group  col-md-6	">
        <input class="input-md form-control" value="@Model.UserResponse.TieneDobleFactor" readonly>
    </div>
</div>