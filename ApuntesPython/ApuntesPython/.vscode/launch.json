    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\Web\\PruebaConceptoDjango\\manage.py",
            "django": true,
            "cwd": "${workspaceFolder}\\Web\\PruebaConceptoDjango\\",
            "args": [
                "runserver"
            ],
            "serverReadyAction": {
                "pattern": ".*(http?:\\/\\/\\S+:[0-9]+\\/?).*",
                "uriFormat": "%s",
                "action": "openExternally"
            }
        },
        {
            "name": "Python: Archivo actual",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${file}"
        }
    ]
}