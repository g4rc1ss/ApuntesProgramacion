<Window x:Class="_1erEjecicio.WindowDock"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="WindowDock" Height="300" Width="300">

    <DockPanel LastChildFill="True">
        <Button MinHeight="60" DockPanel.Dock="Left" x:Name="btnDock" Content="Menu Arriba" Click="BtnDock_Click"   />
        <Button x:Name="btnCerrar" Content="Cerrar" Click="BtnCerrar_Click"   />
    </DockPanel>
</Window>
